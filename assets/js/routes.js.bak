app.config(function($routeProvider) {
  $routeProvider
  .when("/", {
    templateUrl : "view/main.html",
	controller:"myCtrl"
  })
  .when("/about", {
    templateUrl : "view/about.html",
	controller:"myCtrl"
  })
  .when("/contact", {
    templateUrl : "view/contact.html",
	controller:"myCtrl"
  });
});